{
  "name": "wiretap",
  "productName": "Wiretap",
  "version": "1.0.4",
  "description": "Wiretap your mobx observables",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "Raathigesh",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": ["squirrel"],
        "darwin": ["zip"]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "ignore": ["./publish"]
      },
      "electronWinstallerConfig": {
        "name": "Wiretap",
        "loadingGif": "./publish/intro.gif",
        "certificateFile": "./publish/mycert.pfx",
        "setupIcon": "./publish/images/icon.ico"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Raathigesh",
        "name": "wiretap"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Wiretap"
      }
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^1.29.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "cross-env": "^5.0.5",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "2.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.1",
    "font-awesome": "^4.7.0",
    "micro-dev": "^1.2.3",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.3",
    "react": "15.4.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "15.4.1",
    "react-grid-layout": "^0.15.3",
    "react-hot-loader": "3.0.0-beta.6",
    "react-icons": "^2.2.7",
    "react-json-tree": "^0.10.9",
    "react-json-view": "^1.13.0",
    "shortid": "^2.2.8",
    "socket.io": "^1.6.0",
    "socket.io-client": "^2.0.3",
    "spectre.css": "^0.4.3",
    "styled-components": "^2.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.1.2",
    "electron-prebuilt-compile": "1.7.8"
  }
}
