    "babel-preset-stage-1": "^6.24.1",
    "brace": "^0.10.0",
    "classnames": "^2.2.5",
    "electron-log": "^2.2.9",
    "electron-updater": "^2.10.1",
    "express": "^4.16.1",
    "get-port": "^3.2.0",
    "lodash.isstring": "^4.0.1",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.3",
    "mobx-react-devtools": "^4.2.15",
    "moment": "^2.18.1",
    "react": "15.4.1",
    "react-ace": "^5.3.0",
