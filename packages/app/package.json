{
  "name": "wiretap",
  "productName": "Wiretap",
  "main": "./dist/main/index.js",
  "version": "0.4.0",
  "description": "Wiretap your mobx observables",
  "scripts": {
    "dev": " cross-env ELECTRON_WEBPACK_WDS_PORT=9080 electron-webpack dev",
    "compile": "electron-webpack",
    "mac": "npm run compile && node_modules/.bin/build --mac",
